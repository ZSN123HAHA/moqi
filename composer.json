{
    "name": "lhjx/yxx",
    "description": "Yii 2 XX",
    "keywords": ["yii2", "framework", "project template"],
    "homepage": "http://www.yiiframework.com/",
    "type": "project",
    "license": "BSD-3-Clause",
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "~2.0.15",
